{
  "name": "portfolio23",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "next lint --dir . --max-warnings 0",
    "lint:fix": "next lint --dir . --fix --max-warnings 0",
    "check": "npm run format:check && npm run lint",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@radix-ui/react-accessible-icon": "^1.0.3",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/themes": "^2.0.2",
    "@splinetool/react-spline": "^2.2.6",
    "@splinetool/runtime": "^1.0.32",
    "framer-motion": "^10.16.16",
    "geist": "^1.2.0",
    "next": "^14.2.4",
    "next-themes": "^0.2.1",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.7",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.3.3"
  }
}
